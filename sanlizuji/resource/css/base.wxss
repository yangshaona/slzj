/* 公共样式 */

.bg-image {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: -1;
}

.top-bg-image {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9;
    /* clip: rect(0, 371rpx, 80rpx, 0); */
}

.clearfix {
    content: '';
    display: table;
    clear: both;
}